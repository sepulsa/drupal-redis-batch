{
    "name": "drupal/redis_batch",
    "description": "Redis batch storage backend",
    "type": "drupal-module",
    "keywords": [
        "Drupal"
    ],
    "homepage": "https://www.drupal.org/project/redis_batch",
    "readme": "README.md",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Luhur Abdi Rizal",
            "email": "el@elabee.me",
            "homepage": "https://www.drupal.org/u/el7cosmos",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://www.drupal.org/project/issues/redis_batch",
        "source": "http://cgit.drupalcode.org/redis_batch"
    },
    "require": {
        "php": "^7.1",
        "drupal/core": "~8.0",
        "drupal/redis": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^6.5",
        "symfony/phpunit-bridge": "^3.4.3"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\redis_batch\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
